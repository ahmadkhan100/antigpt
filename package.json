{
  "name": "antigpt",
  "version": "1.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "turbo": "turbo",
    "prisma:generate": "turbo run prisma:generate --filter=@antigpt/database",
    "prisma:migrate": "turbo run prisma:migrate --filter=@antigpt/database --",
    "prisma:deploy": "turbo run prisma:deploy --filter=@antigpt/database",
    "prisma:studio": "turbo run prisma:studio --filter=@antigpt/database"
  },
  "private": "true",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "turbo": "^2.5.8"
  }
}
