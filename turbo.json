{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "lint": {
      "outputs": []
    },
    "prisma:generate": {
      "outputs": ["prisma/client/**"]
    },
    "prisma:migrate": {
      "cache": false
    },
    "prisma:deploy": {
      "cache": false
    },
    "prisma:studio": {
      "cache": false
    }
  }
}

